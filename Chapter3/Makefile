all: functions recursion coin_change factorial tail_rec nqueens mut_rec

mut_rec: mut_rec.dats
	acc pc -o mut_rec $<

nqueens: nqueens.dats
	acc pc -o nqueens $<

tail_rec: tail_rec.dats
	acc pc -o tail_rec $<

factorial: factorial.dats
	acc pc -o factorial $<

functions:  functions.dats
	acc pc -o functions $<

recursion: recursion.dats
	acc pc -o recursion $<

coin_change: coin_change.dats
	acc pc -o coin_change $<

clean:
	rm -rf *dats.c functions recursion coin_change factorial tail_rec nqueens mut_rec
